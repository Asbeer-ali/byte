<%-include('../partials/adminPartials/header.ejs') %>
<div style="margin-top: 50px;"></div>
<div>
<h1 style="text-align: center;">Category</h1>
<a href="/admin/addCategory"><button type="button" class="btn btn-outline-success">Add Category</button></a>
<table class="table">
    <thead>
      <tr>
        <th scope="col">No</th>
        <th scope="col">Category</th>
        <th scope="col">Edit</th>
        <th scope="col">Delete</th>
      </tr>
    </thead>
    <% categoryData.forEach((category)=>{%>
    <tbody>
      <tr>
        <th scope="row"><%=i++%></th>
        <td><%=category.categoryName%></td>
        <td><a href="/admin/editCategory/<%=category._id%>"><i class="fa-solid fa-pen" style="color: #000000;"></i></a></td>
        <td><a href="/admin/deleteCategory/<%=category._id%>"><i class="fa-solid fa-trash" style="color: #000000;"></i></a></td>
      </tr>
    </tbody>
      <%})%>
      
  </table>

 </div>
</div>


 <%-include('../partials/adminPartials/footer.ejs') %>